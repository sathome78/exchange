<!-- Modal -->
<div class="modal fade" id="errorInfoSendChatMessageWithoutNickname" tabindex="-1" role="dialog" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLongTitle"><loc:message code="char.error.title.popup"/></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <loc:message code="chat.error.userWithoutNickname"/>
      </div>
      <div class="modal-footer">
        <a href="/settings" class="btn btn-primary btn-md" type="button">
          <loc:message code="merchants.continue"/>
        </a>
        <button class="btn btn-danger btn-md" type="button" data-dismiss="modal">
          <loc:message code="message.close"/>
        </button>
      </div>
    </div>
  </div>
</div>