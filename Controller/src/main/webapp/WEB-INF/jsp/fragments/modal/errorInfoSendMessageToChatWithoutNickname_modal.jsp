<div class="modal fade modal-form-dialog" tabindex="-1" role="dialog" id="error-info-send-chat-message-without-nickname">
  <div style="font-size: 14px" class="modal-dialog modal-md">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title"><loc:message code="char.error.title.popup"/></h4>
      </div>
      <div class="modal-body">
        <div class="errorInfoChat">
          <p><loc:message code="chat.error.userWithoutNickname"/></p>
        </div>
      </div>
      <div class="modal-footer">
          <a href="/settings" id="continue-btn" disabled class="btn btn-primary btn-md" type="button">
            <loc:message code="merchants.continue"/>
          </a>
          <button class="btn btn-danger btn-md" type="button" data-dismiss="modal">
            <loc:message code="message.close"/>
          </button>
      </div>
    </div>
  </div>
</div>