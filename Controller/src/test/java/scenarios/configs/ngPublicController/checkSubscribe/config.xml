<config xmlns="config" active="true" onlyThis="false" patch="default,ieo_subscribe">

    <description>
        ngcontroller package
    </description>

    <get url="/api/public/v2/ieo/subscribe?email=delfin693@yandex.ru">
        <header name="Content-Type" value="application/json"/>

        <response code="200" file="action_1.json"/>

        <comment>
            User subscribed to mail and telegrams.
        </comment>
    </get>

    <get url="/api/public/v2/ieo/subscribe?email=wrongemail@yandex.ru">

        <header name="Content-Type" value="application/json"/>

        <response code="200" file="action_2.json"/>

        <comment>
            User does not exist or did not subscribe to mail and telegrams.
        </comment>
    </get>
</config>
